{
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none",
    "cSpell.words": [
        "dtype",
        "fillna",
        "iloc",
        "iterrows",
        "ndim",
        "STREAMLIT",
        "uform",
        "usearch"
    ],
    "terminal.integrated.profiles.linux": {
        "tmux": {
            "path": "/usr/bin/tmux",
            "args": [
                "new-session",
                "-A",
                "-s",
                "main"
            ]
        }
    },
    "terminal.integrated.defaultProfile.linux": "tmux"
}